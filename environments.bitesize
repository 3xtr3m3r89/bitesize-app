project: kasun-dev
environments:
  - name: production
    namespace: kasun-dev
    deployment:
      method: rolling-upgrade
    services:
      - name: nodedemo-app
        external_url: kasung.dev-bite.io
        port: 80 # this is the port number the application responds on in each container/instance/pod
        ssl: "false"
        replicas: 5
